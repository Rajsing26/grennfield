@using HDFCBankApp.Models;
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@{ 
    List<Customer> customers = (List<Customer>)ViewData["list"];
    foreach(Customer customer in customers)
    {
        <p>@customer.Name @customer.Location  <a href="/customer/details/@customer.Id">Details</a></p>
    }

}

